   <script>

  function loadPage(page) {
    const xhr = new XMLHttpRequest();
    xhr.open('GET', `/userProfile?page=${page}#orders`, true);
    xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');

    xhr.onload = function () {
      if (xhr.status === 200) {

        const parser = new DOMParser();
        const doc = parser.parseFromString(xhr.responseText, 'text/html');

        const newOrders = doc.querySelector('#orders .table-responsive').innerHTML;
        document.querySelector('#orders .table-responsive').innerHTML = newOrders;

       
        const newPagination = doc.querySelector('.pagination').innerHTML;
        document.querySelector('.pagination').innerHTML = newPagination;
        
        history.pushState(null, '', `/userProfile?page=${page}#orders`);
      }
    };

    xhr.send();
  }

</script>


