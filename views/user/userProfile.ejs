<script>

function validateForm(event){

    const currentPassword=document.getElementById('currentPassword').value.trim();
    const newPassword=document.getElementById('newPassword').value.trim();
    const confirmPassword=document.getElementById('confirmPassword').value.trim();



    const passwordPattern = /[a-zA-Z]/;
  const digit = /\d/;
  const specialChar = /[@#\$%\^&\*\(\)_\+\-=\[\]\{\};':"\\|,.<>\/?]+/;
  const uppercase = /[A-Z]/;


  const currentPasswordError = document.getElementById('currentPasswordError');
  const newPasswordError = document.getElementById('newPasswordError');
  const confirmPasswordError = document.getElementById('confirmPasswordError');

  let isValid = true;



   // Reset error messages
   currentPasswordError.innerText = '';
  newPasswordError.innerText = '';
  confirmPasswordError.innerText = '';

  // Validate current password
  if (!currentPassword) {
    currentPasswordError.innerText = 'Current Password is required';
    isValid = false;
  }

  // Validate new password
  if (!newPassword) {
    newPasswordError.innerText = 'New Password is required';
    isValid = false;
  } else if (newPassword.length < 8) {
    newPasswordError.innerText = 'New Password must be at least 8 characters long';
    isValid = false;
  } else if (!passwordPattern.test(newPassword) || !digit.test(newPassword) || !specialChar.test(newPassword) || !uppercase.test(newPassword)) {
    newPasswordError.innerText = 'New Password must contain letters, numbers, special characters, and at least one uppercase letter';
    isValid = false;
  }

  

  // Validate confirm password
  if (!confirmPassword) {
    confirmPasswordError.innerText = 'Confirm Password is required';
    isValid = false;
  } else if (newPassword !== confirmPassword) {
    confirmPasswordError.innerText = 'Passwords must match';
    isValid = false;
  }

  // Prevent form submission if validation fails
  if (!isValid) {
    event.preventDefault();
  }
}


</script>


