        <li class="page-item">
          <button class="page-link" onclick="loadWalletPage(<%= walletCurrentPage - 1 %>)">Previous</button>
        </li>
      <% } %>
  
      <!-- First Page  -->
      <li class="page-item <%= walletCurrentPage === 1 ? 'active' : '' %>">
        <button class="page-link" onclick="loadWalletPage(1)">1</button>
      </li>
  
      <!--  middle page -->
      <% if (walletCurrentPage > 3) { %>
        <li class="page-item disabled">
          <span class="page-link">...</span>
        </li>
      <% } %>
  
      <!--  the Current Page -->
      <% for (let i = Math.max(2, walletCurrentPage - 1); i <= Math.min(walletTotalPages - 1, walletCurrentPage + 1); i++) { %>
        <li class="page-item <%= walletCurrentPage === i ? 'active' : '' %>">
          <button class="page-link" onclick="loadWalletPage(<%= i %>)"><%= i %></button>
        </li>
      <% } %>
  
      <!-- middle range -->
      <% if (walletCurrentPage < walletTotalPages - 2) { %>
        <li class="page-item disabled">
          <span class="page-link">...</span>
        </li>
      <% } %>
  
      <!-- Last Page -->
      <% if (walletTotalPages > 1) { %>
        <li class="page-item <%= walletCurrentPage === walletTotalPages ? 'active' : '' %>">
          <button class="page-link" onclick="loadWalletPage(<%= walletTotalPages %>)"><%= walletTotalPages %></button>
        </li>
      <% } %>
  
      <!-- Next Button -->
      <% if (walletCurrentPage < walletTotalPages) { %>
        <li class="page-item">
          <button class="page-link" onclick="loadWalletPage(<%= walletCurrentPage + 1 %>)">Next</button>
        </li>
      <% } %>
  
    </ul>
  </nav>
