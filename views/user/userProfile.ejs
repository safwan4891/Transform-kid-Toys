                                            <h5 class="mb-0 px-5">Your Orders</h5>
                                        </div>
                                        <div class="card-body">
                                            <!-- Check if there are any orders -->
                                            <% if (orders && orders.length > 0) { %>
                                                <div class="table-responsive">
                                                    <table class="table">
                                                        <thead>
                                                            <tr>
                                                                <th>Order ID</th>
                                                                <th>Date</th>
                                                                <th>Status</th>
                                                                <th>Total</th>
                                                                <th>Details</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <% orders.forEach(order => { %>
                                                                <tr>
                                                                    <td><%= order.orderid %></td>
                                                                    <td><%= order.createdAt.toDateString() %></td>
                                                                    <td><%= order.status %></td>
                                                                    <td><%= order.totalPrice %></td>
                                                                    <td><a href="/orderDetail?id=<%= order._id %>" class="btn-small d-block">View</a></td>
                                                                </tr>
                                                            <% }) %>
                                                        </tbody>
                                                    </table>
                                                </div>
                                
                                                <!-- Pagination - Only show if orders exist -->
                                                <nav aria-label="Page navigation">
                                                    <ul class="pagination justify-content-center">
                                                        <!-- Previous Button -->
                                                        <% if (currentPage > 1) { %>
                                                            <li class="page-item">
                                                                <button class="page-link" onclick="loadPage(<%= currentPage - 1 %>)">Previous</button>
                                                            </li>
                                                        <% } %>
                                
                                                        <!-- First Page -->
                                                        <li class="page-item <%= currentPage === 1 ? 'active' : '' %>">
                                                            <button class="page-link" onclick="loadPage(1)">1</button>
                                                        </li>
                                
                                                        <!-- Dots before current page -->
                                                        <% if (currentPage > 3) { %>
                                                            <li class="page-item disabled">
                                                                <span class="page-link">...</span>
                                                            </li>
                                                        <% } %>
                                
                                                        <!-- Middle Pages -->
                                                        <% for (let i = Math.max(2, currentPage - 1); i <= Math.min(ordersTotalPages - 1, currentPage + 1); i++) { %>
                                                            <li class="page-item <%= currentPage === i ? 'active' : '' %>">
                                                                <button class="page-link" onclick="loadPage(<%= i %>)"><%= i %></button>
                                                            </li>
                                                        <% } %>
                                
                                                       
                                                        <% if (currentPage < ordersTotalPages - 2) { %>
                                                            <li class="page-item disabled">
                                                                <span class="page-link">...</span>
                                                            </li>
                                                        <% } %>
                                
                                                        <!-- Last Page -->
                                                        <% if (ordersTotalPages > 1) { %>
                                                            <li class="page-item <%= currentPage === ordersTotalPages ? 'active' : '' %>">
                                                                <button class="page-link" onclick="loadPage(<%= ordersTotalPages %>)"><%= ordersTotalPages %></button>
                                                            </li>
                                                        <% } %>
                                
                                                        <!-- Next Button -->
                                                        <% if (currentPage < ordersTotalPages) { %>
                                                            <li class="page-item">
                                                                <button class="page-link" onclick="loadPage(<%= currentPage + 1 %>)">Next</button>
                                                            </li>
                                                        <% } %>
                                                    </ul>
                                                </nav>
